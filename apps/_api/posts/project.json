{
  "name": "posts",
  "projectType": "application",
  "sourceRoot": "apps/core/api/posts",
  "targets": {
    "build": {
      "executor": "@nx/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/core/api/posts",
        "main": "apps/core/api/posts/src/main.ts",
        "tsConfig": "apps/core/api/posts/tsconfig.app.json"
      }
    },
    "serve": {
      "executor": "@nx/node:execute",
      "options": {
        "buildTarget": "api:posts:build"
      }
    }
  },
  "tags": ["scope:core", "type:api"]
}
