{
  "name": "rag-engine",
  "projectType": "application",
  "sourceRoot": "apps/core/api/rag-engine",
  "targets": {
    "build": {
      "executor": "@nx/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/core/api/rag-engine",
        "main": "apps/core/api/rag-engine/src/main.ts",
        "tsConfig": "apps/core/api/rag-engine/tsconfig.app.json"
      }
    },
    "serve": {
      "executor": "@nx/node:execute",
      "options": {
        "buildTarget": "api:rag-engine:build"
      }
    }
  },
  "tags": ["scope:core", "type:api"]
}
